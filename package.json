  "author": "Akash Hamirwasia",
  "license": "MIT",
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "dependencies": {
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "downloadjs": "^1.4.7",
    "express": "^4.17.1",
    "preact": "^10.3.2",
    "preact-feather": "^4.1.0",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "scriptjs": "^2.5.9",
    "sucrase": "^3.15.0",
    "ws": "^7.3.0"
  },
  "engines": {
    "node": "10.x"
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "node-sass": "^4.14.1",
    "preact-cli": "^3.0.0",
    "preact-render-spy": "^1.2.1",
    "sass-loader": "^8.0.2",
    "sirv-cli": "^0.4.5",
    "webpack": "^4.43.0"
  }
