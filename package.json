{
  "name": "blaze",
  "version": "1.0.0-beta.3",
  "description": "Quickly share files with multiple users",
  "private": true,
  "scripts": {
    "server": "sucrase-node server/index",
    "dev": "concurrently \"npm:server\" \"preact watch\"",
    "build": "preact build",
    "start": "concurrently \"npm:server\" \"sirv build --cors --single --port 8080 \"",
    "lint": "eslint src utils server"
